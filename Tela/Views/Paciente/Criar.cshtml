@model Tela.Models.Paciente

<h2>Criar Paciente</h2>

<form asp-action="Criar" method="post">
    <div>
        <label asp-for="Nome"></label>
        <input asp-for="Nome" />
    </div>
    <div>
        <label asp-for="CPF"></label>
        <input asp-for="CPF" />
    </div>
    <div>
        <label asp-for="Telefone"></label>
        <input asp-for="Telefone" />
    </div>
    <button type="submit">Criar</button>
</form>
